{
    "name": "colombo/laravel-disk-path-info",
    "description": "",
    "keywords": [
        "colombo",
        "laravel-disk-path-info"
    ],
    "homepage": "https://github.com/colombo/laravel-disk-path-info",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Nguyen Gia Hao",
            "email": "giahao9899@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "laravel/framework": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Colombo\\Libs\\DiskPathTools\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Colombo\\Libs\\DiskPathTools\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Colombo\\Libs\\DiskPathTools\\DiskPathInfoServiceProvider"
            ]
        }
    }
}
